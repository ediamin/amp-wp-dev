{
    "core": "./wordpress-develop/build",
    "phpVersion": "7.2",
    "mappings": {
        "wp-content/plugins/amp": "./amp-wp",
        "../tests/phpunit": "./wordpress-develop/tests/phpunit",
        "../wp-tests-config.php": "./wordpress-develop/wp-tests-config.php"
    },
    "plugins": [
        "https://downloads.wordpress.org/plugin/gutenberg.12.2.0.zip",
        "https://downloads.wordpress.org/plugin/pwa.0.6.0.zip"
    ],
    "config": {
        "WP_DEBUG_LOG": true,
        "WP_DISABLE_FATAL_ERROR_HANDLER": true,
        "WP_AUTO_UPDATE_CORE": false
    },
    "env": {
        "development": {
            "port": 1234
        },
        "tests": {
            "port": 2345
        }
    }
}
